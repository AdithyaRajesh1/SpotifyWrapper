<+>/* Reset some default browser styles */\r\n* {\r\n    margin: 0;\r\n    padding: 0;\r\n    box-sizing: border-box;\r\n}\r\n\r\nbody {\r\n    font-family: Arial, sans-serif;\r\n    line-height: 1.6;\r\n    background-color: #f4f4f4;\r\n    color: #333;\r\n}\r\n\r\n/* Navigation Bar */\r\nnav {\r\n    background-color: #333;\r\n    color: #fff;\r\n    padding: 1em;\r\n}\r\n\r\nnav ul {\r\n    list-style-type: none;\r\n    display: flex;\r\n    justify-content: space-around;\r\n}\r\n\r\nnav ul li {\r\n    display: inline;\r\n}\r\n\r\nnav ul li a {\r\n    color: #fff;\r\n    text-decoration: none;\r\n    font-weight: bold;\r\n    padding: 8px 16px;\r\n    transition: background 0.3s ease;\r\n}\r\n\r\nnav ul li a:hover {\r\n    background-color: #555;\r\n    border-radius: 5px;\r\n}\r\n\r\n/* Main Content Area */\r\n.content {\r\n    max-width: 800px;\r\n    margin: 2em auto;\r\n    padding: 1em;\r\n    background-color: #fff;\r\n    box-shadow: 0px 4px 8px rgba(0, 0, 0, 0.1);\r\n    border-radius: 8px;\r\n}\r\n\r\nh1 {\r\n    text-align: center;\r\n    color: #333;\r\n    margin-bottom: 1em;\r\n}\r\n\r\nh2 {\r\n    color: #444;\r\n    border-bottom: 2px solid #333;\r\n    padding-bottom: 0.2em;\r\n    margin-top: 1.5em;\r\n    margin-bottom: 0.5em;\r\n}\r\n\r\nul {\r\n    list-style-type: none;\r\n    padding: 0;\r\n}\r\n\r\nul li {\r\n    padding: 10px;\r\n    margin: 10px 0;\r\n    border: 1px solid #ddd;\r\n    border-radius: 5px;\r\n    display: flex;\r\n    align-items: center;\r\n    background-color: #fafafa;\r\n}\r\n\r\nul li img {\r\n    border-radius: 5px;\r\n    margin-right: 15px;\r\n}\r\n\r\nul li strong {\r\n    font-size: 1.1em;\r\n}\r\n\r\nul li a {\r\n    color: #1db954;\r\n    text-decoration: none;\r\n    font-weight: bold;\r\n    transition: color 0.3s ease;\r\n}\r\n\r\nul li a:hover {\r\n    color: #14833b;\r\n}\r\n\r\n/* Footer */\r\nfooter {\r\n    text-align: center;\r\n    padding: 1em;\r\n    background-color: #333;\r\n    color: #fff;\r\n    position: fixed;\r\n    bottom: 0;\r\n    width: 100%;\r\n}\r\n\r\ndetails {\r\n    margin-bottom: 1em;\r\n    border: 1px solid #ddd;\r\n    padding: 0.5em;\r\n    border-radius: 5px;\r\n    background-color: #f9f9f9;\r\n}\r\n\r\nsummary {\r\n    font-size: 1.2em;\r\n    font-weight: bold;\r\n    cursor: pointer;\r\n}\r\n\r\nsummary h2 {\r\n    display: inline;\r\n}\r\n\r\ndetails[open] summary {\r\n    color: #1db954;\r\n}\r\n\r\ndetails ul {\r\n    margin-top: 1em;\r\n}\r\n
